    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.70.0",
    "typescript": "~5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.27"
  }
