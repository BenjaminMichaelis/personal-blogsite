    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "~5.5.4",
    "vite": "^4.5.3",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.0.29"
  }
